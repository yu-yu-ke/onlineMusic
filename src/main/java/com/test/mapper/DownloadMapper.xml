<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mapper.DownloadMapper">
	
	<insert id="insert">
		insert into download 
		(user_id,song_id,download_date)
        values 
        (#{userId}, #{songId}, #{downLoadDate})
	</insert>

</mapper>